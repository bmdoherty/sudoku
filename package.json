{
	"name": "sudoku",
	"version": "1.0.6",
	"description": "sudoku",
	"main": "./dist/sudoku.min.js",
	"scripts": {
		"test": "jest --all --coverage ",
		"build": "webpack",
		"precommit": "lint-staged",
		"pretty": "prettier --write"
	},
	"jest": {
		"verbose": true,
		"testURL": "http://localhost/"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"npm run pretty",
			"git add "
		],
		".*rc": [
			"pretty --parser json",
			"git add"
		],
		".*json": [
			"pretty --parser json",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bmdoherty/sudoku.git"
	},
	"author": "bmdoherty",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/bmdoherty/sudoku/issues"
	},
	"homepage": "https://github.com/bmdoherty/sudoku#readme",
	"devDependencies": {
		"@babel/core": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"babel-eslint": "^9.0.0",
		"babel-loader": "^8.0.2",
		"babel-preset-env": "^1.7.0",
		"eslint": "^5.6.0",
		"husky": "^1.0.0-rc.15",
		"jest": "^23.6.0",
		"lint-staged": "^7.3.0",
		"prettier": "^1.14.3",
		"webpack": "^4.19.1",
		"webpack-cli": "^3.1.0"
	},
	"dependencies": {}
}
