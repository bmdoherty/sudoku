{
  "name": "sudoku",
  "version": "1.0.6",
  "description": "sudoku",
  "main": "./dist/sudoku.min.js",
  "scripts": {
    "test": "jest --all --coverage ",
    "build": "webpack",
    "pretty": "prettier --write"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run pretty",
      "git add "
    ],
    ".*rc": [
      "pretty --parser json",
      "git add"
    ],
    ".*json": [
      "pretty --parser json",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmdoherty/sudoku.git"
  },
  "author": "bmdoherty",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmdoherty/sudoku/issues"
  },
  "homepage": "https://github.com/bmdoherty/sudoku#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.3.0",
    "classnames": "^2.2.6"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
